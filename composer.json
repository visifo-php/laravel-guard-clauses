{
    "name": "visifo-php/laravel-guard-clauses",
    "description": "Guard Clauses library for Laravel.",
    "keywords": [
        "visifo-php",
        "laravel",
        "laravel-guard-clauses"
    ],
    "homepage": "https://github.com/visifo-php/laravel-guard-clauses",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergej Tihonov",
            "email": "sergej@visifo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "nunomaduro/larastan": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "visifo-namespace\\Guard\\": "src",
            "visifo-namespace\\Guard\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "visifo-namespace\\Guard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "visifo-namespace\\Guard\\GuardServiceProvider"
            ],
            "aliases": {
                "Guard": "visifo-namespace\\Guard\\Facades\\Guard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
